{
  "name": "simple-web-frontend-template",
  "version": "0.0.1",
  "description": "webpack frontend web website template",
  "scripts": {
    "clean": "rm -Rf ./dist/",
    "pack": "npm run clean && ./node_modules/.bin/webpack --config ./webpack.config.babel.js",
    "lint": "./node_modules/.bin/eslint src tst *.js",
    "lint-quiet": "./node_modules/.bin/eslint --quiet src tst *.js",
    "test": "./node_modules/.bin/mocha --recursive",
    "release": "npm run lint-quiet && npm run pack"
  },
  "keywords": [],
  "author": "Caleb Morris <caleb.morris.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "css-loader": "^0.24.0",
    "eslint": "^3.3.1",
    "eslint-config-honu-base": "^0.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.1",
    "webpack": "^1.13.2"
  }
}
